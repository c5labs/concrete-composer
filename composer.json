{
    "name": "c5labs/concrete5-composer",
    "type": "project",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Green",
            "email": "oliver@c5labs.com"
        }
    ],
    "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/olsgreen/concrete5.git"
    },
    {
        "type": "path",
        "url": "~/Sites/GitHub/c5labs/concrete5-core-installer"
    }
    ],
    "require": {
        "c5labs/concrete5-core-installer": "*",
        "concrete5/concrete5": "dev-core-type"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "installer-paths": {
            "public/concrete": ["type:concrete5-core"],
            "public/application/themes/{$name}": ["type:concrete5-theme"],
            "public/packages/{$name}": ["type:concrete5-package"],
            "public/application/blocks/{$name}": ["type:concrete5-block"]
        },
        "scaffolder": {
            "exclude-paths": [
                "application/bootstrap/autoload.php"
            ]
        }
    }
}
